{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    /* 生产构建时放宽检查 */
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strict": false
  },
  "exclude": [
    "dist",
    "node_modules",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx"
  ]
}